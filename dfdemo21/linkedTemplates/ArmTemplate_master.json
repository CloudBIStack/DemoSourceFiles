{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfdemo21"
		},
		"lsADL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsADL'"
		},
		"lsAzureBatch_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'lsAzureBatch'"
		},
		"lsBatchtest_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'lsBatchtest'"
		},
		"lsBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsBlob'"
		},
		"lsazsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsazsql'"
		},
		"lsAzureBatch_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "azbatch21"
		},
		"lsAzureBatch_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://azbatch21.centralus.batch.azure.com"
		},
		"lsAzureBatch_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "poolhasini"
		},
		"lsBatchtest_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "azbatch2101"
		},
		"lsBatchtest_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://azbatch2101.eastus.batch.azure.com"
		},
		"lsBatchtest_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "bp1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfdemo21_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsADL_connectionString": {
						"value": "[parameters('lsADL_connectionString')]"
					},
					"lsBlob_connectionString": {
						"value": "[parameters('lsBlob_connectionString')]"
					},
					"lsazsql_connectionString": {
						"value": "[parameters('lsazsql_connectionString')]"
					},
					"lsAzureBatch_accessKey": {
						"value": "[parameters('lsAzureBatch_accessKey')]"
					},
					"lsBatchtest_accessKey": {
						"value": "[parameters('lsBatchtest_accessKey')]"
					},
					"lsAzureBatch_properties_typeProperties_accountName": {
						"value": "[parameters('lsAzureBatch_properties_typeProperties_accountName')]"
					},
					"lsAzureBatch_properties_typeProperties_batchUri": {
						"value": "[parameters('lsAzureBatch_properties_typeProperties_batchUri')]"
					},
					"lsAzureBatch_properties_typeProperties_poolName": {
						"value": "[parameters('lsAzureBatch_properties_typeProperties_poolName')]"
					},
					"lsBatchtest_properties_typeProperties_accountName": {
						"value": "[parameters('lsBatchtest_properties_typeProperties_accountName')]"
					},
					"lsBatchtest_properties_typeProperties_batchUri": {
						"value": "[parameters('lsBatchtest_properties_typeProperties_batchUri')]"
					},
					"lsBatchtest_properties_typeProperties_poolName": {
						"value": "[parameters('lsBatchtest_properties_typeProperties_poolName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}