{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdemov222"
		},
		"ADLS_Sales_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_Sales'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"azSqlDB_Adventure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azSqlDB_Adventure'"
		},
		"ADLS_Sales_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saadldemo.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemov222_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_Sales_accountKey": {
						"value": "[parameters('ADLS_Sales_accountKey')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"azSqlDB_Adventure_connectionString": {
						"value": "[parameters('azSqlDB_Adventure_connectionString')]"
					},
					"ADLS_Sales_properties_typeProperties_url": {
						"value": "[parameters('ADLS_Sales_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}