{
	"name": "dsout",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lsadls",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(formatDateTime(utcnow(),'MMddyyyy'),'.zip')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "if(contains($$FileName,'File1.txt','File1'))",
					"type": "Expression"
				},
				"fileSystem": "democontainer"
			},
			"columnDelimiter": ",",
			"compressionCodec": "ZipDeflate",
			"compressionLevel": "Optimal",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}